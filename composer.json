{
    "name": "ellipse/handlers-callable",
    "type": "library",
    "description": "Resolver producing Psr-15 request handlers from callables",
    "keywords": [
        "Psr-15",
        "request-handler",
        "resolver",
        "callable"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/handlers-callable",
    "support": {
        "issues": "https://github.com/ellipsephp/handlers-callable/issues",
        "source": "https://github.com/ellipsephp/handlers-callable"
    },
    "authors": [
        {
            "name": "Pierre Mallinjoud",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0",
        "http-interop/http-server-handler": "^1.0",
        "container-interop/service-provider": "^0.4.0"
    },
    "require-dev": {
        "kahlan/kahlan": "^4.0",
        "eloquent/phony-kahlan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\Handlers\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "binder": [
            {
                "type": "class",
                "value": "Ellipse\\Handlers\\CallableResolverServiceProvider"
            }
        ]
    }
}
